<field_definition level="1" prec="4" operation="average" enabled=".TRUE." default_value="1.e20" > <!-- time step automaticaly defined -->

  <!--
      ============================================================================================================
      Tracer model variables
      ============================================================================================================
  -->

  <!-- ptrc on T grid -->

  <field_group id="bling_T" grid_ref="grid_T_3D">
    <field id="vopo4"        long_name="Dissolved phosphate"        unit="mol/m³" />
    <field id="vodop"         long_name="Dissolved organic phosphate"        unit="mol/m³" /> 
    <field id="vofed"        long_name="Dissolved iron concentration"        unit="mol/m³" />
    <field id="vooxy"         long_name="Dissolved oxygen concentration"        unit="mol/m³" />
    <field id="vodic"        long_name="Dissolved inorganic carbon"        unit="mol/m³" />
    <field id="voalk"         long_name="Total alkalinity concentration"        unit="mol/m³" />
    <field id="vono3"        long_name="Dissolved nitrate"        unit="mol/m³" />
    <field id="vodon"         long_name="Dissolved nitrogen concentration"        unit="mol/m³" />
    <field id="CHL_bling"     long_name="Chlorophyll concentration"    unit="ug/m³" />
  </field_group>

  <field_group id="bling_2d" grid_ref="grid_T_2D">
    <field id="FPOP"         long_name="Sinking particulate P flux"   unit="mol P/m2/s" />
  </field_group>

</field_definition>
